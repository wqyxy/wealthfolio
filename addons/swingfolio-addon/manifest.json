{
  "id": "swingfolio-edited",
  "name": "swingfolio-edited",
  "version": "1.0.1",
  "description": "A Wealthfolio addon for swingfolio-edited",
  "author": "bruce",
  "main": "dist/addon.js",
  "sdkVersion": "2.0.0",
  "enabled": true,
  "permissions": [
    {
      "category": "activities",
      "functions": ["search"],
      "purpose": "Access and manage trading activities for swing trading analysis"
    },
    {
      "category": "portfolio",
      "functions": ["getHoldings"],
      "purpose": "Access portfolio holdings to calculate unrealized P/L for open positions"
    },
    {
      "category": "exchangeRates",
      "functions": ["getAll"],
      "purpose": "Access to exchange rates and currency data"
    },
    {
      "category": "settings",
      "functions": ["get"],
      "purpose": "Access to application settings to get the base currency"
    },
    {
      "category": "ui",
      "functions": ["sidebar.addItem", "router.add"],
      "purpose": "Add navigation and routing for swing trading interface"
    }


  ],
  "keywords": ["wealthfolio", "addon"],
  "license": "MIT"
}
