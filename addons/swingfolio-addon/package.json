{
  "name": "wealthfolio-swingfolio-edited-addon",
  "version": "1.0.1",
  "description": "A Wealthfolio addon for swingfolio-edited",
  "type": "module",
  "main": "dist/addon.js",
  "keywords": [
    "wealthfolio",
    "addon",
    "swing-trading",
    "performance",
    "analytics",
    "trading",
    "calendar"
  ],
    "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "dev:server": "wealthfolio dev",
    "clean": "rm -rf dist",
    "package": "mkdir -p dist && zip -r dist/$npm_package_name-$npm_package_version.zip manifest.json dist/ assets/ README.md",
    "bundle": "pnpm clean && pnpm build && pnpm package",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:quiet": "eslint . --quiet",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.16",
    "@wealthfolio/addon-sdk": "^2.0.0",
    "@wealthfolio/ui": "^2.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^5.0.2",
    "@wealthfolio/addon-dev-tools": "^2.0.0",
    "rollup-plugin-external-globals": "^0.13.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  }
}
